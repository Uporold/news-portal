{
  "name": "webpacktemplate",
  "version": "1.0.0",
  "description": "My template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./build/webpack.dev.conf.js",
    "build": "webpack --config ./build/webpack.build.conf.js",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/assets/scss/**/*.scss\" --syntax sass",
    "test": "npm run editorconfig && npm run stylelint"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "author": "https://github.com/vedees",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedees/webpack-template.git"
  },
  "bugs": {
    "url": "https://github.com/vedees/webpack-template/issues"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "merge",
    "bable",
    "bable7",
    "scss",
    "vue"
  ],
  "license": "MIT",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
